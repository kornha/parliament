{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "postinstall": "node node_modules/puppeteer/install.js",
    "gcp-build": "node node_modules/puppeteer/install.js",
    "serve": "lsof -ti :8085 | xargs -r kill -9; lsof -ti :8080 | xargs -r kill -9; firebase emulators:start --inspect-functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "predeploy": "npm run gcp-build",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "mocha --reporter spec"
  },
  "engines": {
    "node": "20"
  },
  "main": "index.js",
  "dependencies": {
    "@google-cloud/bigquery": "^7.9.0",
    "@google-cloud/firestore": "^7.6.0",
    "@google-cloud/pubsub": "^4.3.3",
    "@pinecone-database/pinecone": "^2.1.0",
    "@polymarket/clob-client": "^4.8.1",
    "cheerio": "^1.0.0",
    "eventregistry": "^9.1.0",
    "firebase": "^10.12.2",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^6.0.1",
    "geofire-common": "6.0.0",
    "html-minifier-terser": "^7.2.0",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "npm": "^10.8.3",
    "openai": "^4.68.1",
    "puppeteer": "^19.11.1",
    "rss-parser": "^3.13.0",
    "uninstall": "^0.0.0",
    "url-metadata": "^3.5.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "axios": "^1.7.2",
    "chai": "^4.4.1",
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "esm": "^3.2.25",
    "firebase-functions-test": "^3.3.0",
    "mocha": "^10.4.0",
    "rewire": "^7.0.0",
    "sinon": "^18.0.0"
  },
  "private": true
}
